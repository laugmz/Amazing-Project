import pandas as pd
import re

def clean_string(s):
    cleaned = re.sub(r'[\[\]\(\)\+\-#]', '', s.lower())  # Clean and convert to lowercase
    return cleaned

def sort_string(s):
    return ''.join(sorted(s.lower()))  # Sort and convert to lowercase

