import pandas as pd

def remove_forward_slash(s):
    """
    Remove forward slash '/' from a string.
    Args:
    - s (str): The input string.
    Returns:
    - cleaned_str (str): The string with forward slash removed.
    """
    return s.replace('(/)', '')

